{
	"name": "ip-echo-cloudflare",
	"version": "1.2.1",
	"description": "A simple no-logs Cloudflare Worker that returns the caller's IP address.",
	"repository": {
		"type": "git",
		"url": "git+https://codeberg.org/AverageHelper/ip-echo-cloudflare.git"
	},
	"keywords": [
		"ip",
		"cloudflare",
		"worker",
		"serverless"
	],
	"author": "AverageHelper",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://codeberg.org/AverageHelper/ip-echo-cloudflare/issues"
	},
	"homepage": "https://codeberg.org/AverageHelper/ip-echo-cloudflare#readme",
	"scripts": {
		"start": "./node_modules/.bin/wrangler dev src/index.ts",
		"test": "./node_modules/.bin/vitest run",
		"test:watch": "./node_modules/.bin/vitest watch --coverage=false",
		"prebuild": "rm -rf ./dist",
		"build": "npm run lint && npm run build:only",
		"build:only": "./node_modules/.bin/tsc",
		"lint": "npm run export-version && ./node_modules/.bin/eslint .",
		"lint:fix": "npm run export-version && npm run lint -- --fix",
		"export-version": "./node_modules/.bin/genversion ./src/meta/version.ts -esd",
		"release": "./node_modules/.bin/ts-node --esm -P tsconfig.test.json ./scripts/release.ts",
		"deploy": "npm run build && ./node_modules/.bin/wrangler deploy",
		"create-release": "./node_modules/.bin/ts-node --esm -P tsconfig.test.json ./scripts/createRelease.ts"
	},
	"type": "module",
	"dependencies": {
		"hono": "3.10.2"
	},
	"devDependencies": {
		"@cloudflare/kv-asset-handler": "0.3.0",
		"@cloudflare/workers-types": "4.20230801.0",
		"@redocly/cli": "1.4.1",
		"@types/node": "20.10.0",
		"@typescript-eslint/eslint-plugin": "5.54.0",
		"@typescript-eslint/parser": "5.54.0",
		"@vitest/coverage-istanbul": "0.34.4",
		"eslint": "8.35.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-config-strictest": "0.6.0",
		"eslint-plugin-deprecation": "1.3.3",
		"eslint-plugin-file-progress": "1.3.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-unicorn": "46.0.0",
		"eslint-plugin-vitest": "0.3.1",
		"genversion": "3.1.1",
		"keep-a-changelog": "2.5.3",
		"semver": "7.5.4",
		"superstruct": "1.0.3",
		"ts-node": "10.9.1",
		"typescript": "4.9.5",
		"vitest": "0.34.3",
		"vitest-environment-miniflare": "2.14.1",
		"wrangler": "3.17.1"
	}
}
